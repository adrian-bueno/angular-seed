{
    "name": "angular-seed",
    "version": "1.0.0",
    "description": "Angular Seed Project",
    "author": "Adrian Bueno (adrian.buenoj@gmail.com)",
    "license": "MIT",
    "scripts": {
        "start": "tsc && concurrently \"tsc -w\" \"lite-server -c=lite-server.config.json\" ",
        "lite": "lite-server -c=lite-server.config.json",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "aot": "ngc -p tsconfig-aot.json",
        "rollup": "rollup -c rollup.config.js",
        "build-prod": "ngc -p tsconfig-aot.json && rollup -c rollup.config.js"
    },
    "dependencies": {
        "@angular/common": "^4.0.1",
        "@angular/compiler": "^4.0.1",
        "@angular/core": "^4.0.1",
        "@angular/forms": "^4.0.1",
        "@angular/http": "^4.0.1",
        "@angular/platform-browser": "^4.0.1",
        "@angular/platform-browser-dynamic": "^4.0.1",
        "@angular/router": "^4.0.1",
        "angular-in-memory-web-api": "~0.3.0",
        "core-js": "^2.4.1",
        "hammerjs": "^2.0.8",
        "jquery": "^3.2.0",
        "material-design-icons": "^3.0.1",
        "materialize-css": "^0.98.1",
        "moment": "^2.17.1",
        "rxjs": "^5.3.0",
        "store": "^2.0.4",
        "systemjs": "^0.20.0",
        "zone.js": "^0.8.5"
    },
    "devDependencies": {
        "@angular/compiler-cli": "^4.0.1",
        "@angular/platform-server": "^4.0.1",
        "@types/core-js": "^0.9.35",
        "@types/hammerjs": "^2.0.34",
        "@types/jquery": "^2.0.40",
        "@types/materialize-css": "^0.98.0",
        "@types/node": "^7.0.0",
        "concurrently": "^3.1.0",
        "lite-server": "^2.2.2",
        "rollup": "^0.41.6",
        "rollup-plugin-commonjs": "^8.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^1.0.1",
        "source-map-explorer": "^1.3.3",
        "typescript": "^2.3.0"
    }
}
